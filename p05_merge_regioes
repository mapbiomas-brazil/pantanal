  var class_reg_3 = ee.Image('projects/mapbiomas-workspace/COLECAO7/classificacao-pantanal/PANT_col7_prob_reg3_v20').select('classification_'+ano)
  var class_reg_4 = ee.Image('projects/mapbiomas-workspace/COLECAO7/classificacao-pantanal/PANT_col7_prob_reg4_v20').select('classification_'+ano)
  var class_reg_5 = ee.Image('projects/mapbiomas-workspace/COLECAO7/classificacao-pantanal/PANT_col7_prob_reg5_v20').select('classification_'+ano)
  var class_reg_6 = ee.Image('projects/mapbiomas-workspace/COLECAO7/classificacao-pantanal/PANT_col7_prob_reg6_v20').select('classification_'+ano)
  var class_reg_7 = ee.Image('projects/mapbiomas-workspace/COLECAO7/classificacao-pantanal/PANT_col7_prob_reg7_v20').select('classification_'+ano)

  var nova_class = ee.ImageCollection.fromImages([class_reg_5,class_reg_4,class_reg_7,class_reg_3,class_reg_6,class_reg_2,class_reg_1]).max()

  var nova_class_reg2 = nova_class.select('classification_'+ano).clip(reg_sem_pasto_SEMPRE)
  nova_class_reg2 = nova_class_reg2.remap([3,4,12,21,22,33],[3,4,12,12,22,33])
  var nova_class_remap = nova_class.select('classification_'+ano).blend(nova_class_reg2)

if (ano <= 1990) {
  var nova_class_reg3 = nova_class.select('classification_'+ano).clip(reg_sem_pasto_1990)
  nova_class_reg3 = nova_class_reg3.remap([3,4,12,21,22,33],[3,4,12,12,22,33])
  nova_class_remap = nova_class.select('classification_'+ano).blend(nova_class_reg3)
  }

  nova_class_remap = nova_class_remap.unmask(nova_class_remap).rename('classification_'+ano)


  if (i_ano == 1){ var class_outTotal = nova_class_remap}//.blend(agric_ano).rename('classification_'+ano) }  
  else {class_outTotal = class_outTotal.addBands(nova_class_remap)}//.blend(agric_ano).rename('classification_'+ano)); }

