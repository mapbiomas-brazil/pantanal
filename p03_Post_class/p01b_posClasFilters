// Define the list of years for processing
var anos = [
            1985,1986,1987,1988,1989,1990,1991,1992,1993,1994,1995,1996,1997,1998,1999,
            2000,2001,2002,2003,2004,2005,2006,2007,2008,2009,2010,2011,
            2012, 2013,2014,2015,2016,2017,2018,2019,2020,2021,2022,2023
            ];

// Create a mask for pixels that are savanna in 2019 and campo in 2023
var finalCampo = col9.select('classification_2018').eq(4).and(col9.select('classification_2023').eq(12)).remap([1],[100]).toByte().selfMask()

// Display the mask
Map.addLayer(finalCampo,{},'finalCampo')

//// Stabilize as pasture 
// If pasture in 1985 and pasture in 2022 and the mode is pasture, then stabilize as pasture
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // If the year is before 2020, use the original classification
    if(ano<2020){
      var saida_ano = col9.select('classification_'+ano)}
    else{
    
    // Correct the classification based on the mask
    var class_corr_agu_ano = col9.select('classification_'+ano).add(finalCampo)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 4, 4,   4, 4,    4, 12, 12, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = col9.select('classification_'+ano).blend(class_corr_agu_ano.rename('classification_'+ano))
    }

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal = saida_ano}  
    else {class_outTotal = class_outTotal.addBands(saida_ano)}
}

// Create a mask for pixels that are campo in 2019 and savanna in 2023
var finalSav = class_outTotal.select('classification_2019').eq(12).and(class_outTotal.select('classification_2023').eq(4)).remap([1],[100]).toByte().selfMask()

// Display the mask
Map.addLayer(finalSav,{},'finalCampo')

//// Stabilize as pasture 
// If pasture in 1985 and pasture in 2022 and the mode is pasture, then stabilize as pasture
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // If the year is before 2020, use the original classification
    if(ano<2020){
      var saida_ano = class_outTotal.select('classification_'+ano)}
    else{
    
    // Correct the classification based on the mask
    var class_corr_agu_ano = class_outTotal.select('classification_'+ano).add(finalSav)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 12, 12, 12, 12, 12, 12, 12, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal.select('classification_'+ano).blend(class_corr_agu_ano.rename('classification_'+ano))
    }

    // Create a mosaic for the current year
    var mosaicoTotal = ee.ImageCollection(asset_mosaicos_col8)
                        .filterMetadata('biome', 'equals', 'PANTANAL')
                        .filterMetadata('year', 'equals', ano)
 //                       .filter(ee.Filter.eq('satellite', asset1))
                        .filterBounds(geometry)
                        .mosaic()

    // Display the mosaic, original classification, and corrected classification
    Map.addLayer(mosaicoTotal, {bands: ['swir1_median', 'nir_median', 'red_median'],gain: [0.08, 0.06, 0.2],gamma: 0.85}, 'mosaico '+ano,false) 

    Map.addLayer(col9.select('classification_'+ano),vis, 'original 9 '+ano,false )
    Map.addLayer(saida_ano,vis, 'Corrigido 29 '+ano,false)

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal2 = saida_ano}  
    else {class_outTotal2 = class_outTotal2.addBands(saida_ano)}
}

// Load the Collection 9 classification image (p03e_v03)
var col9 =  class_outTotal2
Map.addLayer(col9,{},'coleo',false)

// Load auxiliar assets with trajectory images for different classes
var trajetoriaflo = ee.Image('users/gee_arcplan/col9/trajectories_3_comModa')
var npresenceflo = ee.Image('users/gee_arcplan/col9/number_of_presence_3_comModa')
var nchangeflo = ee.Image('users/gee_arcplan/col9/number_of_changes_3_comModa')

var trajetoriasav = ee.Image('users/gee_arcplan/col9/trajectories_4_comModa')
var npresencesav = ee.Image('users/gee_arcplan/col9/number_of_presence_4_comModa')
var nchangesav = ee.Image('users/gee_arcplan/col9/number_of_changes_4_comModa')

var trajetoriaagu = ee.Image('users/gee_arcplan/col9/trajectories_33_comModa')
var npresenceagu = ee.Image('users/gee_arcplan/col9/number_of_presence_33_comModa')
var nchangeagu = ee.Image('users/gee_arcplan/col9/number_of_changes_33_comModa')

var trajetoriacam = ee.Image('users/gee_arcplan/col9/trajectories_12_comModa')
var npresencecam = ee.Image('users/gee_arcplan/col9/number_of_presence_12_comModa')
var nchangecam = ee.Image('users/gee_arcplan/col9/trajectories_12_comModa')

// Define the list of years for processing
var anos = [
            1985,1986,1987,1988,1989,1990,1991,1992,1993,1994,1995,1996,1997,1998,1999,
            2000,2001,2002,2003,2004,2005,2006,2007,2008,2009,2010,2011,
            2012, 2013,2014,2015,2016,2017,2018,2019,2020,2021,2022,2023
            ];

// Where forest presence is greater than savanna presence, then all savanna is forest
var subtractsavFlor = npresenceflo.subtract(npresencesav).gte(0).toByte().selfMask().remap([1],[100]).toByte().selfMask()
Map.addLayer(subtractsavFlor,{},'teste subtract')

// Loop through each year and apply the correction
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the mask
    var class_corr_agr_ano = col9.select('classification_'+ano).add(subtractsavFlor)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 3,   3, 11,12, 19, 21, 22, 25, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = col9.select('classification_'+ano).blend(class_corr_agr_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal = saida_ano}  
    else {class_outTotal = class_outTotal.addBands(saida_ano)}
}

// Any forest trajectory that has water is savanna
var maskPresenceFlo = npresenceflo.gte(1).toByte().selfMask().remap([1],[100]).toByte().selfMask()

var maskPresenceAgu = npresenceagu.gte(1).toByte().selfMask().remap([1],[100]).toByte().selfMask()

var maskAgu1 = maskPresenceFlo.and(maskPresenceAgu).remap([1],[100]).toByte().selfMask()

for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the mask
    var class_corr_agr_ano = class_outTotal.select('classification_'+ano).add(maskAgu1)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 4, 4,   11,12, 19, 21, 22, 25, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal.select('classification_'+ano).blend(class_corr_agr_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal2 = saida_ano}  
    else {class_outTotal2 = class_outTotal2.addBands(saida_ano)}
}

// Where it wasn't forest in 1985 and in other years it was forest, then that forest becomes savanna
var mask85= class_outTotal2.select('classification_1985').neq(3).toByte().selfMask().remap([1],[100]).toByte().selfMask()
//Map.addLayer(mask85,{},'mask85')

for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the mask
    var class_corr_agr_ano = class_outTotal2.select('classification_'+ano).add(mask85)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 4, 4,   11,12, 19, 21, 22, 25, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal2.select('classification_'+ano).blend(class_corr_agr_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal3 = saida_ano}  
    else {class_outTotal3 = class_outTotal3.addBands(saida_ano)}
}

// Any forest with trajectory 6 is savanna
var maskTrajFlo = trajetoriaflo.eq(6).toByte().selfMask().remap([1],[100]).toByte().selfMask()

//Map.addLayer(maskTrajFlo,imageVisParam3,'maskTrajFlo')

for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the mask
    var class_corr_agr_ano = class_outTotal3.select('classification_'+ano).add(maskTrajFlo)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 4, 4,   11,12, 19, 21, 22, 25, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal3.select('classification_'+ano).blend(class_corr_agr_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal4 = saida_ano}  
    else {class_outTotal4 = class_outTotal4.addBands(saida_ano)}
}

// Where savanna trajectory is 6 and presence is less than 6, then all savanna is campo
var maskPresencesav = npresencesav.lt(6).toByte().selfMask().remap([1],[100]).toByte().selfMask()

var maskTrajSav = trajetoriasav.eq(6).toByte().selfMask().remap([1],[100]).toByte().selfMask()

var maskSav2 = maskPresencesav.and(maskTrajSav).remap([1],[100]).toByte().selfMask()
//Map.addLayer(maskSav2,imageVisParam3,'maskSav2')

for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the mask
    var class_corr_agr_ano = class_outTotal4.select('classification_'+ano).add(maskSav2)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 3, 12,   11,12, 19, 21, 22, 25, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal4.select('classification_'+ano).blend(class_corr_agr_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal5 = saida_ano}  
    else {class_outTotal5 = class_outTotal5.addBands(saida_ano)}
}

// Display the corrected classification
Map.addLayer(class_outTotal5,{},'coleo corrigida', false)

// Print the final corrected classification
print(class_outTotal5)

// Define the asset path for the Collection 8 mosaics
var asset_mosaicos_col9 = 'projects/nexgenmap/MapBiomas2/LANDSAT/BRAZIL/mosaics-2';

// Loop through each year and display the mosaic, original classification, and corrected classification
for (var i_ano=0;i_ano<anos.length; i_ano++){
  var ano = anos[i_ano];

  // Create a mosaic for the current year
  var mosaicoTotal = ee.ImageCollection(asset_mosaicos_col9)
                        .filterMetadata('biome', 'equals', 'PANTANAL')
                        .filterMetadata('year', 'equals', ano)
 //                       .filter(ee.Filter.eq('satellite', asset1))
                        //.filterBounds(geometry)
                        .mosaic()

  // Display the mosaic, original classification, and corrected classification
  Map.addLayer(mosaicoTotal, {bands: ['swir1_median', 'nir_median', 'red_median'],gain: [0.08, 0.06, 0.2],gamma: 0.85}, 'mosaico '+ano,false) 
  Map.addLayer(col9.select(['classification_'+ano]),vis,'Original '+ano,false)
  Map.addLayer(class_outTotal5.select(['classification_'+ano]),vis,'corrigido '+ano,false)
}

// Check for savanna pixels in 1985 that are not savanna in 1986
var sav85 = class_outTotal5.select('classification_1985').eq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var nsavk86= class_outTotal5.select('classification_1986').neq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var teste = sav85.and(nsavk86).remap([1],[100]).toByte().selfMask()
Map.addLayer(teste,imageVisParam3,'teste 85')

// Check for savanna pixels in 1986 that are not savanna in 1987
var sav85 = class_outTotal5.select('classification_1986').eq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var nsavk86= class_outTotal5.select('classification_1987').neq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var teste = sav85.and(nsavk86).remap([1],[100]).toByte().selfMask()
Map.addLayer(teste,imageVisParam3,'teste 86')

// Check for savanna pixels in 1987 that are not savanna in 1988
var sav85 = class_outTotal5.select('classification_1987').eq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var nsavk86= class_outTotal5.select('classification_1988').neq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var teste = sav85.and(nsavk86).remap([1],[100]).toByte().selfMask()
Map.addLayer(teste,imageVisParam3,'teste 88')

// Define the band names for the collection
var bandNames = ['classification_1988'
,'classification_1989','classification_1990'
,'classification_1991','classification_1992','classification_1993','classification_1994',
'classification_1995','classification_1996','classification_1997','classification_1998','classification_1999',
'classification_2000','classification_2001','classification_2002','classification_2003','classification_2004','classification_2005'
,'classification_2006','classification_2007','classification_2008','classification_2009','classification_2010','classification_2011',
'classification_2012','classification_2013','classification_2014','classification_2015','classification_2016','classification_2017',
'classification_2018','classification_2019','classification_2020','classification_2021','classification_2022','classification_2023']

// If 1988 is water or campo, then 1987 is campo 
var mask88 = class_outTotal5.select('classification_1988').eq(33).or(class_outTotal5.select('classification_1988').eq(12)).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var sav87 = class_outTotal5.select('classification_1987').eq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var mask87 = sav87.and(mask88).remap([1],[100]).toByte().selfMask()

// Correct the classification based on the mask
var class_corr_agr_ano = class_outTotal5.select('classification_1987').add(mask87)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 12, 12,   11,12, 12, 21, 22, 25, 33])

// Blend the original classification with the corrected classification
var saida_ano = class_outTotal5.select('classification_1987').blend(class_corr_agr_ano.rename('classification_1987'))

// Add the corrected classification to the collection
var final1 = class_outTotal5.select(bandNames).addBands(saida_ano)
//print(final1)

// If 1987 is water or campo, then 1986 is campo
var mask87 = final1.select('classification_1987').eq(33).or(final1.select('classification_1987').eq(12)).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var sav86 = class_outTotal5.select('classification_1986').eq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var mask86 = sav86.and(mask87).remap([1],[100]).toByte().selfMask()

// Correct the classification based on the mask
var class_corr_agr_ano = class_outTotal5.select('classification_1986').add(mask86)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 12, 12,   11,12, 12, 21, 22, 25, 33])

// Blend the original classification with the corrected classification
var saida_ano = class_outTotal5.select('classification_1986').blend(class_corr_agr_ano.rename('classification_1986'))

// Add the corrected classification to the collection
var final2 = final1.addBands(saida_ano)
print(final2)

// If 1986 is water or campo, then 1985 is campo
var mask86 = final2.select('classification_1986').eq(33).or(final2.select('classification_1986').eq(12)).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var sav85 = class_outTotal5.select('classification_1985').eq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()
var mask85 = sav85.and(mask86).remap([1],[100]).toByte().selfMask()

// Correct the classification based on the mask
var class_corr_agr_ano = class_outTotal5.select('classification_1985').add(mask85)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 12, 12,   11,12, 19, 21, 22, 25, 33])

// Blend the original classification with the corrected classification
var saida_ano = class_outTotal5.select('classification_1985').blend(class_corr_agr_ano.rename('classification_1985'))

// Add the corrected classification to the collection
var final3 = final2.addBands(saida_ano)
print(final3)

//temporal filter

var col9 = final3
// Define the asset path for the Collection 8 mosaics
var asset_mosaicos_col8 = 'projects/nexgenmap/MapBiomas2/LANDSAT/BRAZIL/mosaics-2';


// Require the palettes module
var palettes = require('users/mapbiomas/modules:Palettes.js');

// Define visualization parameters
var vis = {
    'min': 0,
    'max': 45,
    'palette': palettes.get('classification5')
}

// Define the list of years for processing
var years = ['1985','1986','1987','1988','1989','1990','1991','1992','1993','1994','1995','1996','1997','1998','1999','2000',
             '2001','2002','2003','2004','2005','2006','2007','2008','2009','2010','2011','2012','2013','2014','2015','2016',
             '2017','2018','2019','2020','2021','2022','2023'];


// Create a mask for pixels that are forest in both 1985 and 2023
var estabiliza = col9.select('classification_1985').eq(3)
                  .and(col9.select('classification_2023').eq(3)).remap([1],[100],0).selfMask()



// Loop through each year and apply the correction
for (var i_year=0;i_year<years.length; i_year++){
  var year = years[i_year];
  
  // Correct the classification based on the mask
  var class_corr_agr_ano = col9.select('classification_'+year).add(estabiliza)
                         .remap([3,4,11,12,19,21,22,25,33,103,104,111,112,119,121,122,125,133],
                                [3,4,11,12,19,21,22,25,33, 3,   3, 3,   3,  3,  3,  3,  3, 3])
  // Blend the original classification with the corrected classification
  var saida_ano = col9.select('classification_'+year).blend(class_corr_agr_ano.rename('classification_'+year))


 
  // Combine the corrected classifications for all years
  if (i_year == 0){ var outotal0 = saida_ano}  
  else {outotal0 = outotal0.addBands(saida_ano)}
}

// Print the combined image
print('outotal0',outotal0)

// Loop through each year and remap the classification
for (var i_year=0;i_year<years.length; i_year++){
  var year = years[i_year];
  

  // Remap the classification band for the current year
  var equivalente_ano = outotal0.select('classification_'+year).remap([3,4,12,21,19,33],[3,4,12,21,19,12]).rename('classification_'+year)
  
  // Combine the remapped classifications for all years
  if (i_year == 0){ var equivalenteCol = equivalente_ano}  
  else {equivalenteCol = equivalenteCol.addBands(equivalente_ano)}
}

// Display the combined remapped image
Map.addLayer(equivalenteCol,{},'equivalenteCol',false)
//var anos = [1993]

// Define the list of years for processing
var anos = [
            1986,1987,1988,1989,1990,1991,1992,1993,1994,1995,1996,1997,1998,1999,
            2000,2001,2002,2003,2004,2005,2006,2007,2008,2009,2010,2011,
            2012, 2013,2014,2015,2016,2017,2018,2019,2020,2021,2022
            ];

// Combine the 1985 and 2023 classification bands
var class_outTotal = equivalenteCol.select('classification_1985').addBands(equivalenteCol.select('classification_2023'))
// Loop through each year and apply the correction
for (var i_ano=0;i_ano<anos.length; i_ano++){
  var ano = anos[i_ano];

  // Create a mask for pixels that are campo in the previous and next year, but not in the current year
  var to_mask_camp = equivalenteCol.select('classification_'+(ano-1)).eq(12)
                .and(equivalenteCol.select('classification_'+ano).neq(12))
                .and(equivalenteCol.select('classification_'+(ano+1)).eq(12)).remap([1],[100],0).toByte().selfMask()    


  // Correct the classification based on the mask
  var class_corr_campv_ano = equivalenteCol.select('classification_'+ano).add(to_mask_camp)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 12,  12,12,12, 12, 12, 12, 33])


//Map.addLayer(class_corr_sav_ano,vis,'teste')



  // Blend the original classification with the corrected classification
  var saida_ano = equivalenteCol.select('classification_'+ano).blend(class_corr_campv_ano.rename('classification_'+ano))

  // Create a mosaic for the current year
  var mosaicoTotal = ee.ImageCollection(asset_mosaicos_col8)
                        .filterMetadata('biome', 'equals', 'PANTANAL')
                        .filterMetadata('year', 'equals', ano)
 //                       .filter(ee.Filter.eq('satellite', asset1))
                        //.filterBounds(geometry)
                        .mosaic()
  // Display the mosaic, original classification, and corrected classification
  Map.addLayer(mosaicoTotal, {bands: ['swir1_median', 'nir_median', 'red_median'],gain: [0.08, 0.06, 0.2],gamma: 0.85}, 'mosaico '+ano,false) 
  Map.addLayer(equivalenteCol.select(['classification_'+ano]),vis,'Original '+ano,false)
  Map.addLayer(saida_ano,vis,'corrigido '+ano,false)

  // Combine the corrected classifications for all years
  if (i_ano == 0){ var class_outTotal2 = saida_ano.addBands(class_outTotal)}  
  else {class_outTotal2 = class_outTotal2.addBands(saida_ano)}
}



// Combine the 1985 and 2023 classification bands
var class_outTotal31 = class_outTotal2.select('classification_1985').addBands(class_outTotal2.select('classification_2023'))
// Loop through each year and apply the correction
for (var i_ano=0;i_ano<anos.length; i_ano++){
  var ano = anos[i_ano];

  // Create a mask for pixels that are forest in the previous and next year, but not in the current year
  var to_mask_flo = class_outTotal2.select('classification_'+(ano-1)).eq(3)
                .and(class_outTotal2.select('classification_'+ano).neq(3))
                .and(class_outTotal2.select('classification_'+(ano+1)).eq(3)).remap([1],[100],0).toByte().selfMask()    


  // Correct the classification based on the mask
  var class_corr_flo_ano = class_outTotal2.select('classification_'+ano).add(to_mask_flo)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 3,   3, 3,   3,  3,  3,  3, 3])


//Map.addLayer(class_corr_sav_ano,vis,'teste')



  // Blend the original classification with the corrected classification
  var saida_ano = class_outTotal2.select('classification_'+ano).blend(class_corr_flo_ano.rename('classification_'+ano))


  // Combine the corrected classifications for all years
  if (i_ano == 0){ var class_outTotal3 = saida_ano.addBands(class_outTotal31)}  
  else {class_outTotal3 = class_outTotal3.addBands(saida_ano)}
}




// Combine the 1985 and 2023 classification bands
var class_outTotal41 = class_outTotal3.select('classification_1985').addBands(class_outTotal3.select('classification_2023'))
// Loop through each year and apply the correction
for (var i_ano=0;i_ano<anos.length; i_ano++){
  var ano = anos[i_ano];

  // Create a mask for pixels that are savanna in the previous and next year, but not in the current year
  var to_mask_sav = class_outTotal3.select('classification_'+(ano-1)).eq(4)
                .and(class_outTotal3.select('classification_'+ano).neq(4))
                .and(class_outTotal3.select('classification_'+(ano+1)).eq(4)).remap([1],[100],0).toByte().selfMask()    


  // Correct the classification based on the mask
  var class_corr_sav_ano = class_outTotal3.select('classification_'+ano).add(to_mask_sav)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 4,   4,  4,  4,  4,  4,  4,  4])


//Map.addLayer(class_corr_sav_ano,vis,'teste')



  // Blend the original classification with the corrected classification
  var saida_ano = class_outTotal3.select('classification_'+ano).blend(class_corr_sav_ano.rename('classification_'+ano))

 

  // Combine the corrected classifications for all years
  if (i_ano == 0){ var class_outTotal4 = saida_ano.addBands(class_outTotal41)}  
  else {class_outTotal4 = class_outTotal4.addBands(saida_ano)}
}


// Create a mask for savanna pixels in 1994 that are not savanna in 1995
var sav85 = col9.select('classification_1994').eq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()

var nsavk86= col9.select('classification_1995').neq(4).toByte().selfMask().remap([1],[100]).toByte().selfMask()

var teste2 = sav85.and(nsavk86).remap([1],[100]).toByte().selfMask()
Map.addLayer(teste2,{},'teste 94')

// Display the original and corrected classifications
Map.addLayer(col9, {}, 'col9', false);
Map.addLayer(class_outTotal4, {}, 'coleo corrigida', false);


// Define variables for exporting the image
var vesion_in = '01'
var version_out = 'p03g_04';
var descricao = 'Filtro janela movel corrige falsas transies de campestre'
var col = 9.0
var prefixo_in = 'PANT_col9_Anual_'
var prefixo_out = 'PANT_col9_Anual_'
var dirout = 'projects/mapbiomas-workspace/COLECAO9/classificacao-pantanal/';


// Set metadata for the classified image
class_outTotal4 = class_outTotal4
.set('territory', 'PANTANAL')
.set('biome', 'PANTANAL')
.set('source', 'arcplan')
.set('version', version_out)
.set('year', version_out)
.set('collection_id', col)
.set('description', descricao)


// Print the final corrected classification
  print('Final exportao:',class_outTotal4)
  // Export the corrected classification to an asset
  Export.image.toAsset({
    "image": class_outTotal4.toByte(),
    "description": prefixo_out+version_out,
    "assetId": dirout + prefixo_out+version_out,
    "scale": 30,
    "pyramidingPolicy": {
        '.default': 'mode'
    },
    "maxPixels": 1e13,
    "region": geometry
  });    
  
  
