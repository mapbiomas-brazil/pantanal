var palettes = require('users/mapbiomas/modules:Palettes.js');
var vis = {
    'min': 0,
    'max': 62,
    'palette': palettes.get('classification7')
};

// Load the Collection 9 classification image (p03c_v10)
var col9 =  ee.Image('projects/mapbiomas-workspace/COLECAO9/classificacao-pantanal/PANT_col9_Anual_p03c_v10')

// Calculate the mode of each pixel across all years
var colModa = ee.ImageCollection([ 
  col9.select('classification_1985').rename('class'),
  col9.select('classification_1986').rename('class'),
  col9.select('classification_1987').rename('class'),
  col9.select('classification_1988').rename('class'),
  col9.select('classification_1989').rename('class'),
  col9.select('classification_1990').rename('class'),
  col9.select('classification_1991').rename('class'),
  col9.select('classification_1992').rename('class'),
  col9.select('classification_1993').rename('class'),
  col9.select('classification_1994').rename('class'),
  col9.select('classification_1995').rename('class'),
  col9.select('classification_1996').rename('class'),
  col9.select('classification_1997').rename('class'),
  col9.select('classification_1998').rename('class'),
  col9.select('classification_1999').rename('class'),
  col9.select('classification_2000').rename('class'),
  col9.select('classification_2001').rename('class'),
  col9.select('classification_2002').rename('class'),
  col9.select('classification_2003').rename('class'),
  col9.select('classification_2004').rename('class'),
  col9.select('classification_2005').rename('class'),
  col9.select('classification_2006').rename('class'),
  col9.select('classification_2007').rename('class'),
  col9.select('classification_2008').rename('class'),
  col9.select('classification_2009').rename('class'),
  col9.select('classification_2010').rename('class'),
  col9.select('classification_2011').rename('class'),
  col9.select('classification_2012').rename('class'),
  col9.select('classification_2013').rename('class'),
  col9.select('classification_2014').rename('class'),
  col9.select('classification_2015').rename('class'),
  col9.select('classification_2016').rename('class'),
  col9.select('classification_2017').rename('class'),
  col9.select('classification_2018').rename('class'),
  col9.select('classification_2019').rename('class'),
  col9.select('classification_2020').rename('class'),
  col9.select('classification_2021').rename('class'),
  col9.select('classification_2022').rename('class'),
  col9.select('classification_2023').rename('class'),
  ]).mode()

// Display the mode image
Map.addLayer(colModa,vis, 'moda col 9')

// Define the list of years for processing
var anos = [
            1985,1986,1987,1988,1989,1990,1991,1992,1993,1994,1995,1996,1997,1998,1999,
            2000,2001,2002,2003,2004,2005,2006,2007,2008,2009,2010,2011,
            2012, 2013,2014,2015,2016,2017,2018,2019,2020,2021,2022,2023
            ];

// Create a mask for water pixels based on the mode image
var maskAgua = colModa.eq(33).remap([1],[100]).toByte().selfMask()

// Display the water mask
Map.addLayer(maskAgua,{},'mscara agua')

//// Stabilize as pasture 
// If pasture in 1985 and pasture in 2022 and the mode is pasture, then stabilize as pasture
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the water mask
    var class_corr_agu_ano = col9.select('classification_'+ano).add(maskAgua)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 12, 12, 12, 12, 12, 12, 12, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = col9.select('classification_'+ano).blend(class_corr_agu_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal = saida_ano}  
    else {class_outTotal = class_outTotal.addBands(saida_ano)}
}

// Create a mask for savanna pixels based on the mode image
var maskSav = colModa.eq(4).remap([1],[100]).toByte().selfMask()

// Display the savanna mask
Map.addLayer(maskSav,{},'mscara sav')

//// Stabilize as pasture 
// If pasture in 1985 and pasture in 2022 and the mode is pasture, then stabilize as pasture
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the savanna mask
    var class_corr_sav_ano = class_outTotal.select('classification_'+ano).add(maskSav)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33,  4,  4,  12, 4, 21, 22, 25, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal.select('classification_'+ano).blend(class_corr_sav_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal2 = saida_ano}  
    else {class_outTotal2 = class_outTotal2.addBands(saida_ano)}
}

// Create a mask for campo pixels based on the mode image
var maskCamp = colModa.eq(12).remap([1],[100]).toByte().selfMask()

// Display the campo mask
Map.addLayer(maskCamp,{},'mscara maskCamp')

//// Stabilize as pasture 
// If pasture in 1985 and pasture in 2022 and the mode is pasture, then stabilize as pasture
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the campo mask
    var class_corr_camp_ano = class_outTotal2.select('classification_'+ano).add(maskCamp)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33,  4,  4,  12, 12, 21, 22, 25, 33])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal2.select('classification_'+ano).blend(class_corr_camp_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal3 = saida_ano}  
    else {class_outTotal3 = class_outTotal3.addBands(saida_ano)}
}

// Print the corrected classification
print('teste',class_outTotal2)

// Display the corrected classification
Map.addLayer(class_outTotal3,{},'coleo corrigido',false)

// Create a mask for pasture pixels based on the mode image and the 1985 and 2023 classifications
var maskPasto = class_outTotal3.select('classification_1985').eq(21)
                .and(class_outTotal3.select('classification_2023').eq(21)).and(colModa.eq(21)).remap([1],[100]).toByte().selfMask()

// Display the pasture mask
Map.addLayer(maskPasto)

//// Stabilize as pasture 
// If pasture in 1985 and pasture in 2022 and the mode is pasture, then stabilize as pasture
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the pasture mask
    var class_corr_agr_ano = class_outTotal3.select('classification_'+ano).add(maskPasto)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 21, 21, 21, 21, 21, 21, 21, 21])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal3.select('classification_'+ano).blend(class_corr_agr_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal4 = saida_ano}  
    else {class_outTotal4 = class_outTotal4.addBands(saida_ano)}
 
}

// Create a mask for forest pixels based on the mode image
var maskFlo = colModa.eq(3).remap([1],[100]).toByte().selfMask()

// Display the forest mask
Map.addLayer(maskFlo,{},'mscara maskFlo')

//// Stabilize as pasture 
// If pasture in 1985 and pasture in 2022 and the mode is pasture, then stabilize as pasture
for (var i_ano=0;i_ano<anos.length; i_ano++){
    var ano = anos[i_ano];
    
    // Correct the classification based on the forest mask
    var class_corr_agr_ano = class_outTotal4.select('classification_'+ano).add(maskFlo)
                         .remap([3,4,12,19,21,22,25,33,103,104,112,119,121,122,125,133],
                                [3,4,12,19,21,22,25,33, 3, 3,   12,  3, 21, 21, 25, 3])

    // Blend the original classification with the corrected classification
    var saida_ano = class_outTotal4.select('classification_'+ano).blend(class_corr_agr_ano.rename('classification_'+ano))

    // Combine the corrected classifications for all years
    if (i_ano == 0){ var class_outTotal5 = saida_ano}  
    else {class_outTotal5 = class_outTotal5.addBands(saida_ano)}
     // Display the original and corrected classifications
     Map.addLayer(col9.select('classification_'+ano),vis, 'original 9 '+ano,false )
     Map.addLayer(saida_ano,vis, 'Corrigido 9 '+ano,false)

}

